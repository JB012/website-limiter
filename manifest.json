{
    "manifest_version": 3,
    "name": "Website Limiter",
    "description": "Base Level Extension",
    "version": "1.0",
    "action": {
      "default_popup": "website.html"
    },
    "host_permissions": [
      "<all_urls>"
    ],
    "permissions": [
      "storage",
      "webNavigation",
      "tabs",
      "notifications"
    ],
    "background": {
      "service_worker": "background.js"
    }
  }